                            <div class="flex flex-wrap overflow-hidden">
                                <div class="w-full rounded shadow-sm">
                                    <div class="flex items-center justify-between mb-4">
                                        <div class="mini-month text-left font-bold text-xl text-black dark:text-white">
                                            {{{display_month}}}
                                        </div>
                                        <div class="mini-year text-left font-bold text-xl text-black dark:text-white">
                                            {{{display_year}}}
                                        </div>

                                    </div>
                                    <div class="-mx-2">
                                        <table class="w-full ">
                                            <tr id="week-days">
                                                {{{render_week_days}}}
                                            </tr>


                                            <tr id="mini-row-1" class="">
                                                {{{render_mini_1 month.row1}}}
                                            </tr>


                                            <tr id="mini-row-2">
                                                {{{render_mini_1 month.row2}}}
                                            </tr>

                                            <tr id="mini-row-3">
                                                {{{render_mini_1 month.row3}}}
                                            </tr>

                                            <tr id="mini-row-4">
                                                {{{render_mini_1 month.row4}}}
                                            </tr>


                                            <tr id="mini-row-5">
                                                {{{render_mini_1 month.row5}}}
                                            </tr>

                                            <tr id="mini-row-6">
                                                {{{render_mini_1 month.row6}}}
                                            </tr>
                                        </table>
                                    </div>
                                </div>
                            </div>
                        </div>

                    </div>
                    {{!-- END CALENDAR DIV --}}



                    {{!-- END DIV FOR TASK AND TIMERS --}}
                </div>


                {{!-- CALENDAR/ MESSAGE DIVS --}}
                <div class="w-full lg:w-10/12 ml-4">



                    {{!-- START MESSAGES DIV --}}
                    <div class="mb-4">
                        {{!-- START MSG --}}
                        <div class="shadow-lg rounded-2xl p-4 bg-white dark:bg-gray-700 w-full">
                            <p class="font-bold text-md text-black dark:text-white underline">
                                Reviews
                            </p>
                            {{!-- LIST --}}
                            <ul>

                                {{#each pet.reviews}}
                                {{!-- <a href="/booking/pet/{{booking_id}}"></a> --}}
                                <li class="flex w-full items-center my-6 space-x-2">
                                    {{!-- INSERT REVIEW HERE --}}

                                    <div class="flex w-full flex-wrap justify-start items-start">
                                        {{>user-review}}

                                        </span>
                                    </div>
                                </li>
                                {{/each}}
                                {{!-- EACH MESSAGE --}}
                                {{#if noMessage}}
                                <li class="flex items-center my-6 space-x-2">

                                    <div class="flex flex-col">
                                        <span class="text-sm text-gray-900 font-semibold dark:text-white ml-2">
                                            No messages!
                                        </span>
                                        <span class="text-sm text-gray-400 dark:text-gray-300 ml-2">

                                        </span>
                                    </div>
                                </li>
                                {{/if}}



                                {{!-- END LIST --}}
                            </ul>

                            {{!-- END MSG --}}
                        </div>

                        {{!-- END MESSAGES DIV --}}
                    </div>


                    {{!-- END DIV FOR CAL AND MESSAGES --}}
                </div>


            </div>


